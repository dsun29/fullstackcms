{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
   "extends": "eslint:recommended",
  "rules": {
    "strict": 1,
    "indent": ["error", "tab"],
    "no-tabs": 0,
    "comma-dangle": ["error", "never"],
    "object-curly-spacing": ["error", "never"],
    "no-console": ["error"]
  }
}